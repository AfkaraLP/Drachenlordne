{
  "manifest_version": 2,
  "name": "Drachenlordne",
  "version": "1.0",
  "description": "Drachenlord your browsing - A Firefox extension to replace all endings of en with ne",
   "permissions": [],
   "icons": {
     "16": "icon16.png",
     "48": "icon48.png",
     "128": "icon128.png"
   },
    "content_scripts": [
      {
        "matches": ["<all_urls>"],
        "js": ["content.js"]
      }
    ],
       "browser_specific_settings": {
         "gecko": {
           "id": "drachenlordne@afkara.dev",
           "data_collection_permissions": {
             "required": ["none"]
           }
         }
       }
 }
